<!--pages/users/users.wxml-->
<import src="../space/space.wxml"/>
<view class="users">
    <view class="head">
        <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
        <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </view>
    <template is="spaceTemplate"></template>
    <view class="my-order">
        <image style="width:12px;height:12px;" mode="aspectFit"  src="../../images/my-order.png"></image>
        <text class="users-text">我的订单</text>
    </view>
    <template is="spaceTemplate"></template>
    <view class="shoping-cart">
        <image style="width:12px;height:12px;" mode="aspectFit" src="../../images/tab/shopingCart.png"></image>
        <text class="users-text">我的订单</text>
    </view>
</view>
